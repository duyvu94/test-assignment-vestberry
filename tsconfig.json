{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts", "sequelize/**/*.js"],
  "exclude": ["node_modules"]
}